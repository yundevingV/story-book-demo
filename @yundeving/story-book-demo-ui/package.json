{
  "name": "@yundeving/story-book-demo-ui",
  "version": "1.2.0",
  "description": "A collection of reusable UI components built with Storybook",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "style.css"
  ],
  "scripts": {
    "build": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist node_modules styled.css",
    "test": "playwright test",
    "test:accessibility": "playwright test tests/accessibility",
    "test:components": "playwright test tests/components",
    "test:visual": "playwright test tests/visual",
    "test:button": "playwright test tests/**/button.spec.ts tests/components/button/**.spec.ts",
    "test:checkbox": "playwright test tests/**/checkbox.spec.ts",
    "test:radiogroup": "playwright test tests/**/radiogroup.spec.ts",
    "test:card": "playwright test tests/accessibility/card/**.spec.ts"
  },
  "keywords": [
    "ui",
    "components",
    "storybook",
    "react",
    "typescript"
  ],
  "author": "Yundeving <ggdbstjd0222@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yundevingV/story-book-demo.git"
  },
  "bugs": {
    "url": "https://github.com/yundevingV/story-book-demo/issues"
  },
  "homepage": "https://github.com/yundevingV/story-book-demo#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "chromatic": "^13.1.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-radio-group": "^1.3.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1"
  },
  "packageManager": "yarn@4.9.0"
}
